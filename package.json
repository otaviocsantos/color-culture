    "plugin",
    "lib",
    "colour"
  ],
  "author": "Otavio Santos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/otaviocsantos/color-culture/issues"
  },
  "homepage": "http://otaviosantos.com/color-culture",
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/node": "^11.15.44",
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "prettier": "^1.19.1",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.19.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@types/mocha": "^8.2.0",
    "ngraph.graph": "0.0.17",
    "ngraph.path": "^1.1.0"
  }
