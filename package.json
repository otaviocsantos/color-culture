{
  "name": "color-culture",
  "version": "5.0.3",
  "description": "Library to convert, link and modify colors",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "mocha -r ts-node/register tests/**/*.spec.ts -- --verbose=true",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "doc": "typedoc --mode modules --readme README.md --out ./docs/ ./src/  ",
    "build": "tsc"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://otaviocsantos@github.com/otaviocsantos/color-culture.git"
  },
  "keywords": [
    "color",
    "culture",
    "conversion",
    "convert",
    "link",
    "relate",
